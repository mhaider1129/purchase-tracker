    "jsdom": "^24.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.16",
    "vite": "^5.4.11",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
